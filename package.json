    "axios": "^0.19.2",
    "axios-oauth-client": "^1.3.0",
    "chart.js": "^2.9.3",
    "core-js": "^3.6.4",
    "install": "^0.13.0",
    "lodash": "^4.17.15",
    "npm": "^6.12.0",
    "typeface-roboto": "0.0.75",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.5.0",
    "vue-router": "^3.1.6",
    "vuetify": "^2.2.22",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@mdi/font": "^5.1.45",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
